{
  "expo": {
    "name": "HOSPICE BIRD",
    "slug": "hopsice-nurse-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hopsice_icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#303544"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hopsice.hopspicecare"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hcare"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
